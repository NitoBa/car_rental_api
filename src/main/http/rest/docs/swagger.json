{
  "openapi": "3.0.0",
  "info": {
    "title": "Car Rental API - Aluguel de Carros",
    "version": "0.0.1"
  },
  "paths": {
    "/account/users/session": {
      "post": {
        "tags": ["Account"],
        "summary": "Login",
        "description": "Login",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "user@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "Test1234."
                  }
                },
                "required": ["email", "password"],
                "example": {
                  "email": "user@gmail.com",
                  "password": "Test1234."
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Authentication success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "user": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string",
                          "example": "6acd1379-8f15-469f-b964-55f45cda308a"
                        },
                        "name": {
                          "type": "string",
                          "example": "User"
                        },
                        "username": {
                          "type": "string",
                          "example": "Username"
                        },
                        "email": {
                          "type": "string",
                          "example": "email@gmail.com"
                        }
                      }
                    },
                    "token": {
                      "type": "string",
                      "example": "asdllksdlasdasdasdasdaakjsda.alskjdasdasdasdoijqowieuqowiuealksjdlakjsdlakjsda.askdjaslkdjalksjdalksjdlasjdausda"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/categories": {
      "post": {
        "tags": ["Category"],
        "summary": "Create a new category",
        "description": "This route is used to create a new category",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "SUV"
                  },
                  "description": {
                    "type": "string",
                    "example": "Categoria de carrros SUV"
                  }
                },
                "required": ["name", "description"],
                "example": {
                  "name": "SUV",
                  "description": "Categoria de carrros SUV"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Category created",
            "content": {
              "application/json": {}
            }
          },
          "400": {
            "description": "Category already exists",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Category already exists"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": ["Category"],
        "summary": "Get all categories",
        "description": "This route is used to get all categories registered",
        "responses": {
          "200": {
            "description": "Categories found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "properties": {
                      "id": {
                        "type": "string",
                        "example": "6acd1379-8f15-469f-b964-55f45cda308a"
                      },
                      "name": {
                        "type": "string",
                        "example": "SUV"
                      },
                      "description": {
                        "type": "string",
                        "example": "Categoria de carrros SUV"
                      },
                      "createdAt": {
                        "type": "string",
                        "example": "2022-04-05T08:40:11.021Z"
                      },
                      "updatedAt": {
                        "type": "string",
                        "example": "2022-04-05T08:40:11.021Z"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/categories/{id}": {
      "put": {
        "tags": ["Category"],
        "summary": "Update a category",
        "description": "This route is used to update a category",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Category ID",
            "required": true,
            "schema": {
              "type": "string",
              "example": "6acd1379-8f15-469f-b964-55f45cda308a"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "SUV"
                  },
                  "description": {
                    "type": "string",
                    "example": "Categoria de carrros SUV"
                  }
                },
                "required": ["name", "description"],
                "example": {
                  "name": "SUV",
                  "description": "Categoria de carrros SUV"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Category updated",
            "content": {
              "application/json": {}
            }
          },
          "404": {
            "description": "Category not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Category not found"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/categories/import": {
      "post": {
        "tags": ["Category"],
        "summary": "Import categories",
        "description": "This route is used to import categories from csv file",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Category imported with success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Categories imported with success"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "File big",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "This file must be smaller than 1MB"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
